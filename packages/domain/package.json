{
  "name": "@sola_mpd/domain",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "npm-run-all --parallel build_*",
    "build:no_types": "npm run build_ts",
    "build_types": "npx tsc",
    "build_ts": "npx swc src -d dist",
    "dev": "npm-run-all --parallel dev_*",
    "dev_types": "npx tsc --watch",
    "dev_ts": "npx swc src -d dist --watch",
    "proto": "rm -rf ./src/models/* && npx buf generate --template buf.gen.yaml ./proto",
    "test": "npx vitest run"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.28.1",
    "@bufbuild/protoc-gen-es": "^1.6.0",
    "@connectrpc/protoc-gen-connect-es": "^1.3.0"
  },
  "dependencies": {
    "fast-deep-equal": "^3.1.3"
  },
  "exports": {
    "./src/*": "./dist/*"
  }
}
