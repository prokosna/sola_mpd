{
	"name": "@sola_mpd/domain",
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"build": "pnpm --parallel /^build_/",
		"build:no_types": "pnpm build_ts",
		"build_types": "tsc",
		"build_ts": "swc src -d dist --strip-leading-paths",
		"dev": "pnpm --parallel /^dev_/",
		"dev_types": "tsc --watch",
		"dev_ts": "swc src -d dist --watch --strip-leading-paths",
		"proto": "rm -rf ./src/models/* && buf generate --template buf.gen.yaml ./proto",
		"test": "vitest run"
	},
	"devDependencies": {
		"@bufbuild/buf": "^1.57.0",
		"@bufbuild/protoc-gen-es": "^2.8.0"
	},
	"dependencies": {
		"fast-deep-equal": "^3.1.3"
	},
	"exports": {
		"./src/*": "./dist/*"
	}
}
